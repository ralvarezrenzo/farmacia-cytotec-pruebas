RewriteOptions inherit
 
<IfModule mod_rewrite.c>
Options -MultiViews
RewriteEngine ON

RewriteCond %{SERVER_PORT} 80 
RewriteRule ^(.*)$ https://huancavelica.farmacias-cytotec-peru.shop/$1 [R,L]
RewriteCond %{HTTP_HOST} ^www\.huancavelica.cytotec\.online$ [NC]  
RewriteRule ^(.*)$ http://huancavelica.farmacias-cytotec-peru.shop/$1 [R=301,QSA,L]

RewriteCond %{REQUEST_FILENAME} -d
RewriteRule (.+) /$1/index.html [QSA,L]

RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule (.+) /$1.html [QSA,L]

</IfModule>
RewriteCond %{HTTP_HOST} ^huancavelica.cytotec\.online$ [OR]
RewriteCond %{HTTP_HOST} ^www\.huancavelica.cytotec\.online$
RewriteRule ^http$ "http\:\/\/https\:\/\/" [R=301,L]

RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

